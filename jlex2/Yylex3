import java.io.*;
class Main{
public static void main(String args[]) throws IOException{
Yylex3 lex=new Yylex3(System.in);
Token token=lex.yylex();
while(token.text!=null) {
token=lex.yylex();
}
}
}
class Token{
String text;
Token(String t){text=t;}
}
%%
%public
%class Yylex3
%type void
digit = [0-9]
letter = [a-zA-Z]
unders = [_]
special = [!@#$%^&*()+]
whitespace = [\t\n]
%type Token
%eofval{
return new Token(null);
%eofval}
%%
"if"
{System.out.println("Keyword" + yytext());}
({letter}|{unders})({letter}|{digit}|{unders})*
{System.out.println("Identifier " + yytext());}
{whitespace}+
{
/*Skip white spaces*/
}

