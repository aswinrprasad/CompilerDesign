import java.io.*;
class Main
{
public static void main(String args[]) throws IOException{
Hello lex=new Hello(System.in);
Token token=lex.yylex();
while(token!=null){
token=lex.yylex();
}
} 
}
class Token{
String text;
Token(String t){
text=t;
}
}
%%
%public 
%class Hello
%type void
digit=[0-9]
letter=[a-zA-Z]
whitespace=[\t\n]
%type Token
%eofval{
return new Token(null);
%eofval}
%%
{letter}({letter}|{digit})*
{
System.out.println("Identifier "+ yytext());}
{whitespace}+
{}